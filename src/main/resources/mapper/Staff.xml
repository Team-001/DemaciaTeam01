<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demacia.mapper.StaffDao">

    <select id="selectStaff" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT
        team01.yj_staff.staff_pwd
        FROM team01.yj_staff
        WHERE team01.yj_staff.staff_name=#{VALUE}
    </select>

</mapper>